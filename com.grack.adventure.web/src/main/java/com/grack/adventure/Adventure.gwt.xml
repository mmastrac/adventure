<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.8.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="adventure">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.common.base.Base" />
	<inherits name="com.google.common.collect.Collect" />
	<inherits name="com.google.common.annotations.Annotations" />
	<inherits name="com.google.gwt.json.JSON" /> 
	<inherits name="javax.annotation.Annotation" />
	
	<inherits name="com.grack.adventure.AdventureLogic" />
	
	<entry-point class='com.grack.adventure.web.Entry' />
	
	<source path="web" />
	
	<set-configuration-property name="user.agent.runtimeWarning" value="false" />
	<set-property name='user.agent' value='safari' />
</module>

